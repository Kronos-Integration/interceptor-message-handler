{
  "name": "kronos-interceptor-message-handler",
  "version": "0.0.0-semantic-release",
  "description": "This interceptor cares about the handling of the messages. It will add the hops and copies the messages",
  "main": "dist/module.js",
  "module": "src/module.js",
  "keywords": [
    "kronos-interceptor"
  ],
  "scripts": {
    "cover": "nyc npm test",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "prepublish": "rollup --output=dist/module.js -c -- src/module.js",
    "pretest": "npm run prepublish",
    "posttest": "markdown-doctest",
    "precover": "npm run prepublish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-interceptor-message-handler.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-interceptor-message-handler/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-interceptor-message-handler#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^1.2.0",
    "jsdoc-to-markdown": "^3.0.0",
    "kronos-test-interceptor": "2.1.0",
    "loglevel-mixin": "^1.6.15",
    "markdown-doctest": "^0.9.1",
    "mocha": "^3.2.0",
    "rollup": "^0.41.4",
    "semantic-release": "^6.3.6",
    "nyc": "^10.0.0"
  },
  "dependencies": {
    "kronos-interceptor": "^2.4.10",
    "kronos-message": "^2.1.0"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=6.9.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  }
}