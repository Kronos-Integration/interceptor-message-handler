{
  "name": "kronos-interceptor-message-handler",
  "version": "0.0.0-semantic-release",
  "main": "dist/message-handler-interceptor.js",
  "module": "src/message-handler-interceptor.js",
  "description": "This interceptor cares about the handling of the messages. It will add the hops and copies the messages",
  "keywords": [
    "kronos-interceptor"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "test": "ava",
    "semantic-release": "semantic-release",
    "docs": "documentation readme src/message-handler-interceptor.js --section=API",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "precover": "rollup -c tests/rollup.config.js",
    "prepare": "rollup -c",
    "lint": "documentation lint src/message-handler-interceptor.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "kronos-interceptor": "^2.4.10",
    "kronos-message": "^2.1.5"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "kronos-test-interceptor": "^3.1.0",
    "loglevel-mixin": "^2.0.2",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.5.0",
    "rollup": "^0.56.5",
    "rollup-plugin-multi-entry": "^2.0.2",
    "semantic-release": "^13.1.4",
    "documentation": "^6.0.0",
    "travis-deploy-once": "^4.3.4"
  },
  "engines": {
    "node": ">=8.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-interceptor-message-handler.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-interceptor-message-handler/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-interceptor-message-handler#readme",
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [
      "env"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
