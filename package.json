{
  "name": "kronos-interceptor-message-handler",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/message-handler-interceptor.js",
  "module": "src/message-handler-interceptor.js",
  "description": "This interceptor cares about the handling of the messages. It will add the hops and copies the messages",
  "keywords": [
    "kronos-interceptor"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "test": "ava",
    "semantic-release": "semantic-release",
    "docs": "documentation readme src/message-handler-interceptor.js --section=API",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "lint": "documentation lint src/message-handler-interceptor.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "kronos-interceptor": "^2.6.0",
    "kronos-message": "^2.2.0"
  },
  "devDependencies": {
    "ava": "^1.0.0-rc.2",
    "kronos-test-interceptor": "^3.1.0",
    "loglevel-mixin": "^2.0.5",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.67.4",
    "semantic-release": "^15.12.5",
    "documentation": "^9.0.0",
    "travis-deploy-once": "^5.0.10",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-executable": "^1.3.0",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-json": "^3.1.0",
    "c8": "^3.2.1",
    "esm": "^3.0.84"
  },
  "engines": {
    "node": ">=10.14.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-interceptor-message-handler.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-interceptor-message-handler/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-interceptor-message-handler#readme",
  "ava": {
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "js",
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
