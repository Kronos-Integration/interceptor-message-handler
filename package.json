{
  "name": "@kronos-integeration/interceptor-message-handler",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/message-handler-interceptor.mjs",
  "exports": {
    ".": "./src/message-handler-interceptor.mjs"
  },
  "description": "This interceptor cares about the handling of the messages. It will add the hops and copies the messages",
  "keywords": [
    "kronos-interceptor"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/message-handler-interceptor.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/message-handler-interceptor.mjs"
  },
  "dependencies": {
    "@kronos-integration/interceptor": "^10.0.11",
    "@kronos-integration/message": "^2.2.0"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "c8": "^7.3.5",
    "documentation": "github:arlac77/documentation",
    "esm": "^3.2.25",
    "loglevel-mixin": "^6.0.0",
    "semantic-release": "^17.2.2"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/interceptor-message-handler.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/interceptor-message-handler/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/interceptor-message-handler#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-github-action",
      "arlac77/template-kronos-component"
    ]
  }
}
