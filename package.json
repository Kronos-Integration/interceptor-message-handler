{
  "name": "kronos-interceptor-message-handler",
  "version": "0.0.0-semantic-release",
  "description": "This interceptor cares about the handling of the messages. It will add the hops and copies the messages",
  "main": "dist/module.js",
  "module": "src/module.js",
  "keywords": [
    "kronos-interceptor"
  ],
  "scripts": {
    "cover": "nyc npm test",
    "test": "mocha tests/*-test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "pretest": "rollup -c",
    "posttest": "markdown-doctest",
    "precover": "rollup -c",
    "prepare": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kronos-Integration/kronos-interceptor-message-handler.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-interceptor-message-handler/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-interceptor-message-handler#readme",
  "devDependencies": {
    "chai": "^4.1.2",
    "jsdoc-to-markdown": "^3.0.0",
    "kronos-test-interceptor": "2.1.0",
    "loglevel-mixin": "^2.0.1",
    "markdown-doctest": "^0.9.1",
    "mocha": "^4.0.1",
    "rollup": "^0.50.0",
    "semantic-release": "^8.2.1",
    "nyc": "^11.2.1"
  },
  "dependencies": {
    "kronos-interceptor": "^2.4.10",
    "kronos-message": "^2.1.0"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=8.7.0"
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/Kronos-Tools/npm-package-template.git"
    }
  },
  "xo": {
    "space": true
  }
}
